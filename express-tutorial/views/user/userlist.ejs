<h1>User List</h1>
<table>
    <thead>
        <th>ID</th>
        <th>Name</th>
        <th>Email</th>
        <th>Password</th>
    </thead>
    <tbody>
        <% for(var i = 0; i < users.length; i++) { %>
            <tr>
                <td>
                    <a href="/user/userdetail/<%= users[i]._id %>"><%= users[i]._id %></a>
                </td>
                <td><%= users[i].name %></td>
                <td><%= users[i].email %></td>
                <td><%= users[i].password %></td>
            </tr>
        <%}%>
    </tbody>
</table>
